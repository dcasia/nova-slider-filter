{
    "name": "digital-creative/nova-slider-filter",
    "description": "A Laravel Nova filter for picking range between a min/max value.",
    "keywords": [
        "laravel",
        "nova",
        "slider",
        "filter",
        "range"
    ],
    "authors": [
        {
            "name": "Rafael Milewski"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\NovaRangeFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCreative\\NovaRangeFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
